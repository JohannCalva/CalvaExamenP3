<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="CalvaExamenP3.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:CalvaExamenP3"
    xmlns:views="clr-namespace:CalvaExamenP3.Views"
    Shell.FlyoutBehavior="Disabled"
    Title="CalvaExamenP3">
    <TabBar>
        <Tab Title="Agregar Contacto">
            <ShellContent ContentTemplate="{DataTemplate views:AgregarContactoPage}" />
        </Tab>
        <Tab Title="Registros">
            <ShellContent ContentTemplate="{DataTemplate views:RegistrosPage}" />
        </Tab>
        <Tab Title="Logs">
            <ShellContent ContentTemplate="{DataTemplate views:LogsPage}" />
        </Tab>
    </TabBar>
</Shell>
